    "postcss-sass": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint"
    ]
  },
  "gacp": {
    "push": true,
    "emoji": "code"
  },
