{
  "name": "lens-ui",
  "version": "5.0.0",
  "source": "src/index.ts",
  "main": "dist/index.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "description": "LensUI Design System",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production && npm run check:licenses && npm run types",
    "start:dev": "run-p start:dev:source start:dev:types --print-label --race",
    "start:dev:source": "webpack --watch --progress --mode development",
    "start:dev:types": "tsc src/index.ts --declaration --allowJs --emitDeclarationOnly --outDir dist/types --esModuleInterop --jsx react --resolveJsonModule --skipLibCheck --watch",
    "test": "tsc --noEmit && jest --coverage --silent",
    "check:licenses": "license-checker --production --csv --out dist/licenses.csv",
    "test:dev": "jest --watchAll --coverage",
    "lint": "npm run lint:code && npm run lint:styles",
    "types": "tsc src/index.ts --declaration --allowJs --emitDeclarationOnly --outDir dist/types --esModuleInterop --jsx react --skipLibCheck",
    "lint:code": "eslint ./src --ext .tsx --ext .ts --max-warnings=0 --fix",
    "lint:styles": "stylelint \"src/**/*.scss\" --fix",
    "release:minor": "standard-version --release-as minor && npm run build && git push --follow-tags origin main && npm publish",
    "release:major": "standard-version --release-as major && npm run build && git push --follow-tags origin main && npm publish",
    "release:patch": "standard-version --release-as patch && npm run build && git push --follow-tags origin main && npm publish",
    "release:pre": "standard-version --prerelease && npm run build && git push --follow-tags origin main && npm publish --tag next",
    "prepare": "husky install",
    "storybook": "start-storybook -p 3003",
    "deploy-storybook": "build-storybook && storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancaetano/lens-ui.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Typescript",
    "Design System",
    "UX",
    "UI",
    "Storybook"
  ],
  "author": "Lucian Caetano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luciancaetano/lens-ui/issues"
  },
  "homepage": "https://github.com/luciancaetano/lens-ui#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/react": "==1.6.22",
    "@namics/stylelint-bem": "^7.0.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.9",
    "@storybook/storybook-deployer": "^2.8.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-select": "^5.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "hygen": "^6.2.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-useragent-mock": "^0.1.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-sass": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint"
    ]
  },
  "gacp": {
    "push": true,
    "emoji": "code"
  },
  "dependencies": {
    "@floating-ui/core": "^1.0.0",
    "@floating-ui/dom": "^1.0.0",
    "@floating-ui/react-dom": "^1.0.0",
    "@floating-ui/react-dom-interactions": "^0.9.1",
