{
  "name": "lens-ui",
  "version": "4.9.2",
  "source": "src/index.ts",
  "main": "dist/index.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "description": "LensUI Design System",
  "scripts": {
    "build": "rimraf dist/ && cross-env NODE_ENV=production webpack --mode production && npm run check:licenses && npm run types",
    "test": "tsc --noEmit && jest --coverage --silent",
    "check:licenses": "license-checker --production --csv --out dist/licenses.csv",
    "test:dev": "jest --watchAll --coverage",
    "lint": "npm run lint:code && npm run lint:styles",
    "types": "tsc src/index.ts --declaration --allowJs --emitDeclarationOnly --outDir dist/types --esModuleInterop --jsx react --skipLibCheck",
    "lint:code": "eslint ./src --ext .tsx --ext .ts --max-warnings=0 --fix",
    "lint:styles": "stylelint \"src/**/*.scss\" --fix",
    "release:minor": "standard-version --release-as minor && npm run build && git push --follow-tags origin main && npm publish",
    "release:major": "standard-version --release-as major && npm run build && git push --follow-tags origin main && npm publish",
    "release:patch": "standard-version --release-as patch && npm run build && git push --follow-tags origin main && npm publish",
    "release:pre": "standard-version --prerelease && npm run build && git push --follow-tags origin main && npm publish --tag next",
    "prepare": "husky install",
    "storybook": "start-storybook -p 3003",
    "deploy-storybook": "build-storybook && storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancaetano/lens-ui.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Typescript",
    "Design System",
    "UX",
    "UI",
    "Storybook"
  ],
  "author": "Lucian Caetano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luciancaetano/lens-ui/issues"
  },
  "homepage": "https://github.com/luciancaetano/lens-ui#readme",
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@mdx-js/react": "==1.6.22",
    "@namics/stylelint-bem": "^7.0.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.1",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-select": "^5.0.0",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "hygen": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.0.2",
    "jest-useragent-mock": "^0.1.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.4.1",
    "loader-utils": "^3.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-sass": "^0.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint"
    ]
  },
  "gacp": {
    "push": true,
    "emoji": "code"
  },
  "dependencies": {
    "@floating-ui/core": "^0.6.2",
    "@floating-ui/dom": "^0.4.5",
    "@floating-ui/react-dom": "^0.6.3",
    "@fontsource/roboto": "^4.5.5",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@wojtekmaj/react-daterange-picker": "^3.4.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.1",
    "lodash": "^4.17.21",
    "react-currency-input": "^1.3.6",
    "react-element-popper": "^2.1.6",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-input-mask": "^2.0.4",
    "react-multi-date-picker": "^3.3.0",
    "react-select": "^5.3.1",
    "react-spring": "^9.4.4",
    "react-tooltip": "^4.2.21",
    "regenerator-runtime": "^0.13.9",
    "reset-css": "^5.0.1",
    "sweetalert2": "^11.4.9",
    "use-debounce": "^7.0.1"
  }
}
